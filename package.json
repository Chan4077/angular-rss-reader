{
  "name": "rss-reader",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "githubPages": "npm run genDocs && ng deploy --base-href=/rss-reader/ && rm -rf dist/",
    "githubPages2": "npm run build && ngh && rm -rf dist/",
    "genDocs": "gulp docs",
    "build": "ng build --prod --base-href /rss-reader/ --deploy-url /rss-reader/",
    "build-prod-local": "ng build -c=httpserver --prod && cd dist && http-server",
    "deploy": "ng deploy",
    "action:deploy": "npm run genDocs && ng deploy --repo https://x-access-token:GH_TOKEN@github.com/EdricChan03/rss-reader.git --no-silent --name $GITHUB_ACTOR --email $GITHUB_ACTOR@users.noreply.github.com"
  },
  "private": true,
